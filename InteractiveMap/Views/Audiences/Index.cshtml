@model IEnumerable<InteractiveMap.Models.Audience>
@{ViewData["Title"] = "Index"; }
<h1>Index</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SvgCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.aud)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.imgWay)
            </th>
            <th>
                @Html.DisplayNameFor(model=>model.imgSvg)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SvgCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.aud)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.imgWay)
            </td>
            <td>
                @Html.DisplayFor(modelItem=>item.imgSvg)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
